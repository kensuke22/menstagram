<h1>投稿フォーム</h1>

<%= form_with model: @ramen_image, local:true do |f| %>
  <h4>ラーメン写真</h4>
  <%= f.attachment_field :image %>
  <h4>店舗名</h4>
  <%= f.text_field :shop_name %>
  <h4>住所</h4>
  <%= f.text_field :address %>
  <h4>税込価格</h4>
  <%= f.text_field :tax_price %>
  <h4>スープ</h4>
  <%= f.text_field :taste %>
  <h4>感想</h4>
  <%= f.text_area :caption %>
  
  <%= f.submit '投稿' %>
<% end %>
  