
  <% @users.each do |user| %>
       
    <%= link_to user_path(user) do %>
      <%= attachment_image_tag user, :profile_image, :fill, 80, 80, fallback: "no_image.jpg" %>
    <% end %>
               
    <div class="title"><%=link_to user.username, user_path(user) %></div>
              
  <% end %>
  